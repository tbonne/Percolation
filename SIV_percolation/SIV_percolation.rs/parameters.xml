<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<parameter name="randomSeed" displayName="Default Random Seed" type="int" defaultValue="__NULL__" />
	
	<parameter name="numberOfGroups" displayName="1. Number of groups" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		
		<parameter name="groupSize" displayName="2. Size of groups" type="int" 
						defaultValue="15" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		
		<parameter name="landscapeSize" displayName="3. Size of the landscape (m)" type="int" 
						defaultValue="1000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		
		<parameter name="maxRadiusOfConnections" displayName="4. Max connection distance amoung groups (m)" type="int" 
						defaultValue="150" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		<parameter name="dispersalP" displayName="5. Dispersal probability of individuals" type="double" 
						defaultValue="0.01" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		<parameter name="pTrans" displayName="6. Probability of transmission (%/y)" type="double" 
						defaultValue="0.25" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		
		
		

</parameters>
